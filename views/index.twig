{% extends 'layout/default.twig' %}

{% block content %}
	<h1>{{ title }}</h1>
	{% for post in posts %}
		{% include ['_partials/tease-'~post.post_type~'.twig', '_partials/tease.twig'] %}
	{% endfor %}
	{% include '_partials/pagination.twig' with { pagination: posts.pagination({show_all: false, mid_size: 3, end_size: 2}) } %}
{% endblock %}
